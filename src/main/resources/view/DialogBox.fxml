<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<fx:root alignment="TOP_RIGHT" maxHeight="Infinity" maxWidth="Infinity" minHeight="-Infinity" prefWidth="-Infinity" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="dialog" text="Label" textFill="#393b3c">
         <font>
            <Font name="SansSerif Bold" size="13.0" />
         </font></Label>
        <ImageView fx:id="displayPicture" fitHeight="68.0" fitWidth="67.0" pickOnBounds="true" preserveRatio="true">
         <HBox.margin>
            <Insets left="10.0" right="10.0" />
         </HBox.margin></ImageView>
    </children>
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
    </padding>
</fx:root>
